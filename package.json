{
  "name": "react-discrete-slider",
  "version": "1.0.0",
  "description": "A simple slider made with React",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --devtool eval-source --progress --colors",
    "build": "webpack --config webpack-prod.config.js  --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emadb/react-discrete-slider.git"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Emanuele DelBono (@emadb)",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "errno": "^0.1.4",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "node-sass": "^3.3.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
